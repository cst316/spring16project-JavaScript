import javax.swing.JButton;

import org.junit.After;
import org.junit.Assert;
import org.junit.Before;
import org.junit.Test;

import net.sf.memoranda.ui.WorkPanel;

public class WorkPanelTest {

	private String clickType;
	
	@Test
	public void testClick() {
		Assert.assertTrue("It is not enabled", WorkPanel.agendaB.isEnabled());
		Assert.assertTrue("It is not enabled", WorkPanel.tasksB.isEnabled());
		Assert.assertTrue("It is not enabled", WorkPanel.eventsB.isEnabled());
		Assert.assertTrue("It is not enabled", WorkPanel.filesB.isEnabled());
		Assert.assertTrue("It is not enabled", WorkPanel.pspB.isEnabled());
		Assert.assertTrue("It is not enabled", WorkPanel.pspBLearn.isEnabled());
		
	}

}
